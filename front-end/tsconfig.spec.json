{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine",
      "node"
    ]
  },
  "files": [
    "src/test.ts",
    "src/polyfills.ts"
    
  ],
  "include": [
    "src/**/*.spec.ts",
    "src/**/*.d.ts",
    "test/**/*.spec.ts",
    "../front-end/test/*.spec.ts",
    "/test/",
    "./test/*.spec.ts"
  ]
}
